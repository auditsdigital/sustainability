    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/geoip-lite": "^1.1.31",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^26.0.0",
    "@types/memoizee": "^0.4.4",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/puppeteer": "^2.0.1",
    "@types/uuid": "^7.0.2",
    "characterset": "^1.3.0",
    "css-tree": "^1.0.0-alpha.39",
    "fastify": "^2.15.0",
    "fastify-static": "^2.7.0",
